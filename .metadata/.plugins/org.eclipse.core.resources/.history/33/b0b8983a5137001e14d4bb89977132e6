package di_p.main;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import di_p.MobilePhone;

public class MobilePhone_main {

    public static void main(String[] args) {

        AbstractApplicationContext context =
                new ClassPathXmlApplicationContext("di_xml/mobile_wire.xml");

        System.out.println("adultphone : " + context.getBean("adultphone"));
        System.out.println("phone2 : " + context.getBean("phone2"));
        System.out.println("phone3 : " + context.getBean("phone3"));
        System.out.println("phone4 : " + context.getBean("phone4"));
    }
}
