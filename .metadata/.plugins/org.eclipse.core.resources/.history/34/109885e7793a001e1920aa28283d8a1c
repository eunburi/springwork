package anno_p.coffebean;

import org.springframework.stereotype.Component;

@Component
public class Makchi {

	
	Water wt;
	String name = "라떼";
	caramel cm;
	
	
	
	
	public void setWt(Water wt) {
		this.wt = wt;
	}




	public void setName(String name) {
		this.name = name;
	}




	public void setCm(caramel cm) {
		this.cm = cm;
	}




	@Override
	public String toString() {
		return "Makchi [wt=" + wt + ", name=" + name + ", cm=" + cm + "]";
	}




	
	
}
