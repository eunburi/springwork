package anno_p;

import org.springframework.context.annotation.Bean;

import anno_p.com.Bonche;
import anno_p.com.WorkStation;
import anno_p.com.sub1.Mouse;
import anno_p.comabc.NoteBook;

public class Computers {

	@Bean //메소드를 이용
	Mouse hp920() { //본체
		return new Mouse("HP920","버티컬");
	}
	
	@Bean
	NoteBook gm1() {
		return new NoteBook();
	}
	
	@Bean
	WorkStation ws1() {
		return new WorkStation("델1",null,null,null,hp920());
		
	}
	
	@Bean // 이게 더 강력쓰
	WorkStation ws2(Bonche, asus, Mouse hp920) { // 매개변수로 bean호출
		return new WorkStation("델2",null,null,null,hp920);
		
	}
	
	
}
