package anno_p;

import jakarta.annotation.Resource;

public class RCCar {

	String name;
	@Resource
	Motor mtr;
	RCwhell rw1,rw2,rw3,rw4;
	
	
}

class RCwhell{
	
	String name;
	int size;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setSize(int size) {
		this.size = size;
	}
	
	@Override
	public String toString() {
		return "RCwhell [name=" + name + ", size=" + size + "]";
	}
	
	
}


