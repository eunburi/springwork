package anno_p.main;

import org.springframework.context.support.AbstractApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

public class Required_main {

	public static void main(String[] args) {
		
		AbstractApplicationContext context = 
				new ClassPathXmlApplicationContext("anno_xml/qcoffe_required.xml");

		
		System.out.println(context.getBean("am1"));
		System.out.println(context.getBean("am2"));
		System.out.println(context.getBean("am3"));
		System.out.println(context.getBean("am4"));
		System.out.println(context.getBean("am5"));
		
		context.close();
		
				
	}

}
