package anno_p.coffebean;

import org.springframework.stereotype.Component;

@Component
public class Makchi {

	
	Water wt;
	String name = "라떼";
	
	
	public void setWt(Water wt) {
		this.wt = wt;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	@Override
	public String toString() {
		return "Latte [wt=" + wt + ", name=" + name + "]";
	}
	
	
}
