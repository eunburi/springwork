package di_p;

public class MobilePhone {
	String name;
	MobileBattery bat;
	MobileMonitor mon;
	MobileCamera cam;
	
	public MobilePhone() {
		System.out.println("MobilePhone 기본생성자");
	}
	


	public MobilePhone(String name, MobileBattery bat, MobileMonitor mon, MobileCamera cam) {
		super();
		this.name = name;
		this.bat = bat;
		this.mon = mon;
		this.cam = cam;
		System.out.println("MobilePhone 생성자 1");
	}

	

	
	public void setName(String name) {
		this.name = name;
	}



	public void setBat(MobileBattery bat) {
		this.bat = bat;
	}



	public void setMon(MobileMonitor mon) {
		this.mon = mon;
	}



	public void setCam(MobileCamera cam) {
		this.cam = cam;
	}



	@Override
	public String toString() {
		return "MobilePhone [name=" + name + ", bat=" + bat + ", mon=" + mon + ", cam=" + cam + "]";
	}
	
	
	
	
	
	
}





class MobileBattery{
	String name;
	int power;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPower() {
		return power;
	}
	public void setPower(int power) {
		this.power = power;
	}
	
	@Override
	public String toString() {
		return "MobileBattery [name=" + name + ", power=" + power + "]";
	}
	
	

	
	
	
	
}

class MobileMonitor{
	String name;
	int size;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	@Override
	public String toString() {
		return "MobileMonitor [name=" + name + ", size=" + size + "]";
	}
	
	
	
}

class MobileCamera{
	String name, type;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	@Override
	public String toString() {
		return "MobileCamera [name=" + name + ", type=" + type + "]";
	}
	
	
	
	
}
